{
	"name": "storyteller-suite",
	"version": "1.5.5",
	"description": "A comprehensive suite for managing characters, locations, events, and galleries for your novels.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -p tsconfig.json --noEmit && node esbuild.config.mjs production",
		"postinstall": "node fix-expect-webdriverio.js",
		"test": "vitest run --reporter=default",
		"test:watch": "vitest",
		"test:e2e": "cross-env PATH=\"%PATH%;C:\\Program Files\\7-Zip\" wdio run wdio.conf.mjs",
		"test:e2e:watch": "cross-env PATH=\"%PATH%;C:\\Program Files\\7-Zip\" wdio run wdio.conf.mjs --watch",
		"test:ai": "playwright test -c test/playwright-ai/playwright.config.ts",
		"test:ai:bug-finder": "playwright test -c test/playwright-ai/playwright.config.ts test/playwright-ai/specs/autonomous-bug-finder.spec.ts",
		"test:ai:autonomous": "playwright test -c test/playwright-ai/playwright.config.ts test/playwright-ai/specs/autonomous-bug-finder.spec.ts",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@midscene/web": "^1.2.1",
		"@playwright/test": "^1.57.0",
		"@types/cytoscape": "^3.21.0",
		"@types/js-yaml": "^4.0.9",
		"@types/leaflet": "^1.9.21",
		"@types/leaflet-draw": "^1.0.13",
		"@types/leaflet.markercluster": "^1.5.6",
		"@types/luxon": "^3.7.1",
		"@types/node": "18.19.39",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"@vitest/coverage-v8": "1.6.0",
		"@wdio/cli": "^9.5.0",
		"@wdio/globals": "^9.5.0",
		"@wdio/local-runner": "^9.5.0",
		"@wdio/mocha-framework": "^9.5.0",
		"@wdio/spec-reporter": "^9.5.0",
		"@wdio/types": "^9.5.0",
		"builtin-modules": "3.3.0",
		"chrono-node": "2.6.4",
		"cross-env": "^10.1.0",
		"cytoscape": "^3.30.0",
		"dotenv": "^17.2.3",
		"esbuild": "^0.25.12",
		"js-yaml": "^4.1.1",
		"luxon": "3.4.4",
		"obsidian": "^1.8.7",
		"obsidian-launcher": "^1.1.0",
		"playwright": "^1.57.0",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"vis-timeline": "7.7.0",
		"vitest": "1.6.0",
		"wdio-obsidian-service": "^1.0.0"
	},
	"engines": {
		"node": ">=18.18.0 <23"
	},
	"dependencies": {
		"leaflet": "^1.9.4",
		"leaflet-draw": "^1.0.4",
		"leaflet.markercluster": "^1.5.3",
		"timeline-arrows": "^4.8.0"
	},
	"optionalDependencies": {
		"@esbuild/linux-x64": "^0.27.2"
	}
}
